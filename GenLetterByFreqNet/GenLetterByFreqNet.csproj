<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFrameworks>net7.0</TargetFrameworks>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<OutputType>Library</OutputType>
		<PackageId>GenLetterByFreqNet</PackageId>
		<Version>4.0.1</Version>
		<Authors>Cown877</Authors>
		<Description>
			Randomly Generate Characters Based Off Of Character Frequency Value.
			Characters With Higher Freqeuncy Percent Values Are More Likely To Be
			Randomly Generated.
		</Description>
	</PropertyGroup>
</Project>